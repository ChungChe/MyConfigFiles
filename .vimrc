colorscheme huntex
set bs=2
set wildmenu
syntax on
set hlsearch
set shiftwidth=4
set softtabstop=4
set expandtab
set tabstop=4
set nu
set autoindent
set cindent
set cinoptions=l1,g0
set nowrap
set mouse=n
set ruler
set t_Co=256
set laststatus=2
set statusline=\ [%t]
set statusline+=\ %y
set statusline+=%=
set statusline+=%l,
set statusline+=%c/%L
set statusline+=\ %P\ \ \

map <F5>    :tabnew<CR><bar>:Explore<CR>
